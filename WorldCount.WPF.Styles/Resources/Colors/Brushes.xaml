<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">


    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Colors/Colors.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="TransparentBrush" Color="{StaticResource Transparent}" p:Freeze="true"/>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}" p:Freeze="true"/>
    <SolidColorBrush x:Key="LightBlackBrush" Color="{StaticResource LightBlack}" p:Freeze="true"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}" p:Freeze="true"/>

    <SolidColorBrush x:Key="RedBrush" Color="{StaticResource Red}" p:Freeze="true"/>
    <SolidColorBrush x:Key="GreenBrush" Color="{StaticResource Green}" p:Freeze="true"/>
    <SolidColorBrush x:Key="BlueBrush" Color="{StaticResource Blue}" p:Freeze="true"/>
    <SolidColorBrush x:Key="GrayBrush" Color="{StaticResource Gray}" p:Freeze="true"/>
    <SolidColorBrush x:Key="OrangeBrush" Color="{StaticResource Orange}" p:Freeze="true"/>
    <SolidColorBrush x:Key="DarkOrangeBrush" Color="{StaticResource DarkOrange}" p:Freeze="true"/>
    
</ResourceDictionary>